package com.nepharm.apps.fpp.biz.mr.bean;

public class SbdaBean {
	
	/**设备编号*/
	private String sbbh;
	
	/**设备名称*/
	private String sbmc;
	
	/**规格型号*/
	private String ggxh;
	
	/**状态*/
	private String zt;
	
	/**资产编号 */
	private String zcbh;
	
	/**部门全路径名称*/
	private String bmallpathName;

	public SbdaBean(String sbbh, String sbmc, 
			String ggxh, String zt, String zcbh, String bmallpathName) {
		this.sbbh = sbbh;
		this.sbmc = sbmc;
		this.ggxh = ggxh;
		this.zt = zt;
		this.zcbh = zcbh;
		this.bmallpathName = bmallpathName;
	}

	public String getSbbh() {
		return sbbh;
	}

	public void setSbbh(String sbbh) {
		this.sbbh = sbbh;
	}

	public String getSbmc() {
		return sbmc;
	}

	public void setSbmc(String sbmc) {
		this.sbmc = sbmc;
	}

	public String getGgxh() {
		return ggxh;
	}

	public void setGgxh(String ggxh) {
		this.ggxh = ggxh;
	}

	public String getZt() {
		return zt;
	}

	public void setZt(String zt) {
		this.zt = zt;
	}

	public String getBmallpathName() {
		return bmallpathName;
	}

	public void setBmallpathName(String bmallpathName) {
		this.bmallpathName = bmallpathName;
	}

	public String getZcbh() {
		return zcbh;
	}

	public void setZcbh(String zcbh) {
		this.zcbh = zcbh;
	}
	
	

}
